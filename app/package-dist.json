{
  "name": "millix",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.html",
  "node-main": "node-main.js",
  "window": {
    "icon": "./icon.png",
    "width": 1260,
    "height": 620,
    "min_width": 1260,
    "min_height": 480,
    "resizable": true
  },
  "scripts": {
    "start": "nw ."
  },
  "author": "developer@millix.org",
  "license": "MIT",
  "chromium-args": "--ignore-certificate-errors --mixed-context",
  "js-flags": "",
  "node-remote": [
    "<all_urls>"
  ],
  "//": "product_string: millix",
  "devDependencies": {
  },
  "build": {
    "nwVersion": "0.66.0",
    "nwFlavor": "sdk",
    "strippedProperties": [
      "scripts",
      "devDependencies",
      "build"
    ],
    "outputPattern": "${NAME}-${PLATFORM}-${ARCH}",
    "packed": false,
    "win": {
      "icon": "icon.ico"
    },
    "mac": {
      "icon": "icon.icns",
      "documentIcon": "icon.icns"
    }
  },
  "dependencies": {
    "sqlite3": "^5.0.8"
  }
}
